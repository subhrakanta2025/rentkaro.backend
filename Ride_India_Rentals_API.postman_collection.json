    {
      "name": "Uploads",
      "item": [
        {
          "name": "Upload Files (GCS)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}"
              }
            ],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "files",
                  "type": "file",
                  "src": ""
                }
              ]
            },
            "url": {
              "raw": "{{base_url}}/uploads",
              "host": ["{{base_url}}"],
              "path": ["uploads"]
            },
            "description": "Upload files (images, pdfs) to Google Cloud Storage. Requires GCS_BUCKET and a valid service account. The API returns public URLs for uploaded files."
          },
          "response": []
        }
      ]
    },
